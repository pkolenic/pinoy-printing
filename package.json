{
  "name": "pinoy-printing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:frontend": "dotenv -- npm start --prefix frontend",
    "start:backend": "dotenv -- npm run dev --prefix backend",
    "start": "NODE_ENV=production npm run start:frontend",
    "start:dev": "NODE_ENV=development npm run start:frontend & NODE_ENV=development npm run start:backend",
    "install-all": "npm install --prefix frontend && npm install --prefix backend",
    "build:frontend": "npm run build --prefix frontend",
    "build:backend": "npm run build --prefix backend",
    "build:all": "npm run build:frontend && npm run build:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkolenic/pinoy-printing.git"
  },
